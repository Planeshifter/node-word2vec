{
  "name": "word2vec",
  "version": "0.8.0",
  "description": "node.js interface to the Google word2vec tool",
  "main": "lib/main.js",
  "scripts": {
    "test": "mocha",
    "postinstall": "make --directory=src",
    "test-cov": "istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coverage -- -R spec",
    "coveralls": "istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coveralls/coverage --report lcovonly -- -R spec && cat ./reports/coveralls/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./reports/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Planeshifter/node-word2vec.git"
  },
  "keywords": [
    "text-analytics",
    "deep-learning",
    "word-distance"
  ],
  "author": {
    "name": "Philipp Burckhardt"
  },
  "license": "APA-2",
  "bugs": {
    "url": "https://github.com/Planeshifter/node-word2vec/issues"
  },
  "homepage": "https://github.com/Planeshifter/node-word2vec",
  "dependencies": {
    "change-case": "^2.1.5",
    "charm": "^1.0.0",
    "mime": "^1.2.11",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "chai": "3.x.x",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.8",
    "mocha": "2.x.x"
  }
}
